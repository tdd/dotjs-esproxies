    "@slidev/theme-default": "^0.25.0",
    "@slidev/theme-seriph": "^0.25.0",
    "tpyo": "^2.0.0"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/mdi": "^1.2.3",
    "@types/react": "^19.0.12",
    "playwright-chromium": "^1.51.1",
    "react": "^19.0.0",
    "rimraf": "^6.0.1",
    "ts-expect": "^1.3.0"
  }
}
